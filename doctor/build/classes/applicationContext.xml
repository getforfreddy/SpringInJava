<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"

xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

xmlns:p="http://www.springframework.org/schema/p"

xsi:schemaLocation="http://www.springframework.org/schema/beans

http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
<bean class="bean.Doctor" id="Doc">
<constructor-arg value="1" type="int"></constructor-arg>
<constructor-arg value="Vaishakh"></constructor-arg>
<constructor-arg value="25000.50" type="double"></constructor-arg>
<constructor-arg value="orthopedic doctor"></constructor-arg>
</bean>

<bean class="bean.Doctor" id="Doct">
<property name="id" value="102"></property>
<property name="name" value="Prince Robin"></property>
<property name="salary" value="45005.25"></property>
<property name="specialization" value="General"></property>
</bean>

</beans>